{
  "name": "react-gallery",
  "version": "1.0.0",
  "description": "A react plugin for maintaining all static assets for any node project (nextJS, CRA...).",
  "files": [
    "resources",
    "scripts",
    "io/dist",
    "io/package.json",
    "io/README.md",
    "view/build",
    "view/package.json",
    "view/README.md",
    "CHANGELOG"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "start-io": "nodemon io/dist/index.js",
    "start-io:prod": "nodemon io/dist/index.js",
    "build-io": "npm --prefix io run build",
    "start-view": "npm --prefix view run start",
    "start-view:prod": "http-server view/build -p 5003",
    "build-view": "npm --prefix view run build",
    "start-app:prod": "npm run start-io:prod & npm run start-view:prod",
    "build-app": "npm run build-io && npm run build-view",
    "start-app:dev": "npm --prefix io run watch & npm run start-io & npm run start-view"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abusayid693/react-gallery.git"
  },
  "keywords": [
    "react-gallery"
  ],
  "bin": {
    "start-gallery": "./scripts/bin.js"
  },
  "author": "Rehane",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Abusayid693/react-gallery/issues"
  },
  "homepage": "https://github.com/Abusayid693/react-gallery#readme",
  "dependencies": {
    "http-server": "^14.1.1",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "husky": "^7.0.0",
    "prettier": "^1.19.1",
    "prettierrc": "^0.0.0-5"
  }
}
